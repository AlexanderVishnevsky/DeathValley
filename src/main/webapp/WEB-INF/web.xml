<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <listener>
        <listener-class>listener.DatabaseContextLIstener</listener-class>
    </listener>

    <servlet>
        <servlet-name>GetUserController</servlet-name>
        <display-name>GetUserController</display-name>
        <description></description>
        <servlet-class>controller.GetUserController</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetSumController</servlet-name>
        <display-name>GetSumController</display-name>
        <description></description>
        <servlet-class>controller.GetSumController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetUserController</servlet-name>
        <url-pattern>/getRichestUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSumController</servlet-name>
        <url-pattern>/getSum</url-pattern>
    </servlet-mapping>
</web-app>
